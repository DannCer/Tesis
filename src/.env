# ============================================================================
# CONFIGURACIÓN DE GEOSERVER
# ============================================================================

# URL base de tu servidor GeoServer (sin /geoserver al final)
VITE_GEOSERVER_URL=http://localhost:8080

# Workspace de GeoServer donde están tus capas
VITE_GEOSERVER_WORKSPACE=Tesis

# Timeout para peticiones WFS (milisegundos)
VITE_WFS_TIMEOUT=30000

# Máximo número de features a cargar
VITE_MAX_FEATURES=5000

# ============================================================================
# CONFIGURACIÓN DEL MAPA
# ============================================================================

# Centro del mapa (México)
VITE_MAP_CENTER_LAT=23.5
VITE_MAP_CENTER_LNG=-102.5

# Niveles de zoom
VITE_MAP_ZOOM=5.5
VITE_MAP_MIN_ZOOM=5
VITE_MAP_MAX_ZOOM=18

# ============================================================================
# CONFIGURACIÓN DE LA APLICACIÓN
# ============================================================================

VITE_APP_NAME=Geovisor - Monitoreo de Uso de Suelo
VITE_APP_VERSION=1.0.0

# Modo debug (activa logs en consola)
VITE_DEBUG_MODE=true
